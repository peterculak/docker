{
    "name": "behat/website-skeleton",
    "type": "project",
    "license": "MIT",
    "description": "A skeleton to start a new Symfony website",
    "require": {
        "php": "^7.1.3"
    },
    "require-dev": {
        "behat/behat": "~2.5",
        "behat/mink-extension": "~1.3",
        "behat/mink-goutte-driver": "~1.2",
        "behat/mink-selenium2-driver": "~1.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "vendor-dir": "./vendor/"
    }
}
